{
  "version": "1.0",
  "update_interval": 3600,
  "sensors": {
    "temperature": {
      "enabled": true,
      "pin": 4,
      "threshold": 25
    },
    "humidity": {
      "enabled": true,
      "pin": 5,
      "threshold": 60
    }
  },
  "reporting": {
    "endpoint": "http://your-future-ingest-api.com/data",
    "batch_size": 10
  }
}
